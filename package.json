    "hbs": "~4.0.4",
    "highlight.js": "^9.17.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "babel-preset-minify": "^0.5.1",
    "babelify": "^10.0.0",
    "cross-env": "^6.0.3",
